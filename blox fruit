	Name = "Refresh Weapon",
	Callback = function()
		Weapons:Clear()
        for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetChildren()) do
            if v:IsA"Tool" then
                Weapons:UpdateList({
                    Items = {v.Name},
                    Replace = true
                })
            end
        end
	end
})

